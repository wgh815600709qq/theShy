{
    "compilerOptions": {
      "baseUrl": "./",
      "paths": {
        "ts-ui": [
          "components/index.tsx"
        ],
        "ts-ui/es/*": [
          "components/*"
        ]
      },
      "allowJs": true,
      "resolveJsonModule": true,
      "declaration": true,
      "strictNullChecks": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "jsx": "react",
      "noUnusedParameters": true,
      "noUnusedLocals": true,
      "noImplicitAny": true,
      "target": "es6",
      "lib": [
        "dom",
        "es2017"
      ],
      "skipLibCheck": true,
      "typeRoots": [
        "node_modules/@types"
      ]
    },
    "exclude": [
      "node_modules",
      "lib",
      "es",
      "docs"
    ]
  }
  